function dna_find_pair($input) {
    if ($input == 'A') {
        return 'T';
    } elseif ($input == 'T') {
        return 'A';
    } elseif ($input == 'C') {
        return 'G';
    } elseif ($input == 'G') {
        return 'C';
    } else {
        return ' ';
    }
}

